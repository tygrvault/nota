{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
      "path:default",
      "event:default",
      "window:default",
      "app:default",
      "image:default",
      "resources:default",
      "menu:default",
      "tray:default",
      "shell:allow-open",
      "fs:default",
      "fs:read-all",
      "fs:write-all",
      "store:default",
      "store:allow-get",
      "store:allow-set",
      "store:allow-save",
      "store:allow-load",
      "store:allow-entries",
      "store:allow-delete",
      "store:allow-reset",
      "dialog:default",
      "dialog:allow-open",
      "store:default"
  ]
}